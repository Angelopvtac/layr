{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [".env", ".env.local"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "dist/**", "!dist/**/*.tsbuildinfo"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {},
    "test": {
      "outputs": [],
      "dependsOn": ["build"]
    },
    "type-check": {
      "outputs": [],
      "dependsOn": ["build"]
    },
    "clean": {
      "cache": false
    }
  }
}